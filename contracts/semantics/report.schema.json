{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://semantah.com/contracts/semantics/report.schema.json",
  "title": "SemReport",
  "type": "object",
  "required": [
    "kind",
    "created_at"
  ],
  "properties": {
    "kind": {
      "type": "string"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "notes": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "stats": {
      "type": "object"
    }
  }
}
